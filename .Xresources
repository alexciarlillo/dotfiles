URxvt.scrollBar: false
URxvt.font: xft:source code pro:style=Medium:pixelsize=11
URxvt.boldFont: xft:fira mono:style=Medium:pixelsize=11
URxvt.perl-ext-common: default,matcher,clipboard
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1
URxvt.keysym.Home: \033[7~
URxvt.keysym.End: \033[8~
URxvt.keysym.KP_Home: \033[7~
URxvt.keysym.KP_End:  \033[8~

